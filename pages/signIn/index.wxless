.list-wrap {
  margin: 24rpx 0;
  box-sizing: border-box;
  .signed-box {
    margin: 0 24rpx;
    padding: 24rpx;
    border: 1px solid #02c874;
    margin-top: 24rpx;
    box-sizing: border-box;
  }
  .room-name {
    color: #333;
    font-size: 28rpx;
  }
  .signed-tag {
    color: rgb(91, 133, 119);
    font-size: 22rpx;
    margin-left: 60rpx;
  }
}
