.class-item {
  background: rgb(241, 241, 241);
  margin: 20rpx;
  box-sizing: border-box;
  padding: 24px 0;
  view {
    padding: 12rpx;
    box-sizing: border-box;
  }
  .copy-btn {
    width: 300rpx;
    border-radius: 12rpx;
    border: 1px solid #02c874;
    text-align: center;
    padding: 8rpx;
    margin: 0 auto;
  }
}
.create-class {
  border: 1px solid #acc;
  border-radius: 24rpx;
  padding: 24rpx;
}
