/* components/navigation/navigation.wxss */
.navigation-wrapper {
  position: fixed;
  display: flex;
  align-items: center;
  width: 100%;
  top: 0;
  left: 0;
  height: 45px;
  overflow: hidden;
  z-index: 999;
}
.navigation-wrapper .icon-group {
  margin-left: 15px;
  align-items: center;
  width: 50px;
  height: 100%;
  box-sizing: border-box;
  display: flex;
  overflow: hidden;
}
.navigation-wrapper .icon-group .icon-item {
  width: 50%;
}
.navigation-wrapper .title-wrapper {
  width: calc(100% - 130px);
  line-height: 45px;
  height: 45px;
  text-align: left;
  font-weight: 500;
  font-size: 32rpx;
  color: #FFFFFF;
  text-align: center;
}
.empty {
  height: 45px;
  width: 100%;
}
